JS CALBACK ASYNC AWAIT 

// to set timeout code
function getData(dataId, getNextData) {
  setTimeout(() => {
    console.log("data", dataId);
    if (getNextData) getNextData();
  }, 3000); // 3 second delay
}

getData(1, () => {
  getData(2, () => {
    getData(3, () => {
      getData(4);
    });
  });
});

console.log("1");
setTimeout(() => console.log("2"), 3000);
setTimeout(() =>console.log("3"), 3000);
console.log("3")







